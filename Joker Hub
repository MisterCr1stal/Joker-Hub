if game.placeId == 920587237 then -- Adopt Me
print("ADOPT ME GUI LOADED!")
loadstring(game:HttpGet("https://raw.githubusercontent.com/MisterCr1stal/Joker-Hub/main/Adopt%20Me%20Script", true))()

elseif game.placeId == 2512643572 then -- Bubble Gum Simulator
print("BUBBLE GUM GUI LOADED!")
loadstring(game:HttpGet("https://raw.githubusercontent.com/MisterCr1stal/Joker-Hub/main/Bubble%20Gum", true))()

elseif game.placeId == 142823291 then -- Murder Mystery 2
print("MURDER MYSTERY 2 GUI LOADED!")
loadstring(game:HttpGet("https://raw.githubusercontent.com/MisterCr1stal/Joker-Hub/main/Murder%20Mystery%202", true))()


else
game.Players.LocalPlayer:Kick("Joker Hub not support this game!")
end
