-- Scripts:

local function WPUNT_fake_script() -- NA.LocalScript 
	local script = Instance.new('LocalScript', NA)

	local player = game.Players.LocalPlayer
	
	if player then
		script.Parent.Text = "Hello!"..player.Name.."!"
	end
end
coroutine.wrap(WPUNT_fake_script)()



wait(5)
JokerHub.step1:Destroy()
JokerHub.step2.Visible = true
wait(2)
JokerHub.step2:Destroy()
JokerHub.step3.Visible = true
wait(5)
JokerHub.step3:Destroy()
JokerHub.step4.Visible = true
wait(2)
JokerHub.step4:Destroy()
wait(1)
JokerHub.Main.Visible = true
